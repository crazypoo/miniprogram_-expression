.image_detail_box {
    width: 100%;
    height: 100%;
    display: flex;
    margin-top: 10rpx;
    justify-content: center;
}

.image_detail_head {
    margin-top: 15rpx;
    display: flex;
    flex-direction: column;
}

.image-detail-kuan {
    width: 300rpx;
    height: 300rpx;
}

.iamge_btn_group {
    display: flex;
    justify-content: center;
}

.ico_btn_image {
    height: 80rpx;
    width: 80rpx;
}

.grids {
    margin-top: 20rpx;
    position: relative;
    width: 100%;
    display: flex;
    flex-wrap: wrap;
}

.grid::after,.grid::before,.grids::after,.grids::before {
    content: "";
    display: block;
    position: absolute;
    z-index: 99;
    background-color: #ccc;
}

.grid::before,.grids::before {
    left: 0;
    width: 100%;
    height: 1px;
    transform: scaleY(.5);
}

.grids::before {
    top: 0;
}

.grid::before {
    bottom: 0;
}

.grid::after,.grids::after {
    top: 0;
    height: 100%;
    width: 1px;
    transform: scaleX(.5);
}

.grids::after {
    left: 0;
}

.grid::after {
    right: 0;
}

.grid {
    box-sizing: border-box;
    position: relative;
    display: flex;
    flex-wrap: wrap;
    align-items: center;
    justify-content: space-around;
    width: 25%;
    height: 150rpx;
    padding: 5rpx;
    text-align: center;
}

.grid:active {
    background-color: #fafafa;
}

.grid-icon {
    margin-top: 10rpx;
    margin-bottom: 10rpx;
}

.grid-icon,.grid-icon-share {
    width: 80rpx;
    height: 80rpx;
    background-position: 50%;
    background-repeat: no-repeat;
    background-size: contain;
}

.grid-icon-share {
    margin-top: 16rpx;
    margin-left: 10rpx;
    margin-bottom: 0rpx;
}

.grid-title {
    width: 100%;
    color: #666;
    font-size: 25rpx;
}

.grid-icon-favorite {
    background-image: url(data:image/png;base64,);
}

.search-bar {
    box-sizing: border-box;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 999;
    width: 100%;
    height: 90rpx;
    padding: 10rpx;
    background-color: #ffda44;
    text-align: center;
    box-shadow: 0 10rpx 60rpx rgba(0,0,0,.3);
}

.search-txt {
    display: inline-block;
    width: 730rpx;
    height: 70rpx;
